all:
	cd lib && $(MAKE)
	if [ @has_fortran@ = yes ] ; then cd libf && $(MAKE) ; fi
	cd utils && $(MAKE)

distclean:
	cd lib && $(MAKE) distclean
	cd libf && $(MAKE) distclean
	cd utils && $(MAKE) distclean
	rm -f Makefile

clean:
	cd lib && $(MAKE) clean
	cd libf && $(MAKE) clean
	cd utils && $(MAKE) clean

install:
	cd lib && $(MAKE) install
	if [ @has_fortran@ = yes ] ; then cd libf && $(MAKE) install ; fi
	cd utils && $(MAKE) install
