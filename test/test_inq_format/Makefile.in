srcdir  = @srcdir@
VPATH   = @srcdir@

include ../../macros.make

INCLUDES  = -I../../src/lib -I$(srcdir)/../common
LDFLAGS  += -L../../src/lib -L../common
LIBS     += -ltestutils -lpnetcdf

SRCS    = test_inq_format.c 

OBJS    = $(SRCS:.c=.o)
PROGS   = $(SRCS:.c=)

GARBAGE      = $(PROGS)
PACKING_LIST = $(SRCS) Makefile.in

all: $(PROGS)
	set -e; cd ../data && $(MAKE) all

test_inq_format: test_inq_format.o $(LIBRARY)
	$(LINK.c) $< $(LDFLAGS) $(LIBS)

include $(srcdir)/../../rules.make

