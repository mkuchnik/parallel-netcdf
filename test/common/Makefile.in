srcdir = @srcdir@
VPATH  = @srcdir@

include ../../macros.make

INCLUDES  = -I. -I$(srcdir)

SRCS    = testutils.c

OBJS    = $(SRCS:.c=.o)

PROGS   = libtestutils.a

GARBAGE      = $(PROGS)
PACKING_LIST = $(SRCS) Makefile.in

all: $(PROGS)

libtestutils.a: $(OBJS)
	$(AR) $(ARFLAGS) $@ $<

testutils.o: testutils.c testutils.h

include $(srcdir)/../../rules.make

